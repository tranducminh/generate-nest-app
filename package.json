{
  "name": "generate-nest-boilerplate",
  "version": "0.0.22",
  "description": "This generator will help you build your own NestJS app with CQRS, TypeORM, MySQL, Redis, Typescript, fastify, S3",
  "author": "Tran Duc Minh",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:setup:local": "docker-compose --env-file .env up -d",
    "db:setup:dev": "docker-compose --env-file .env.development up -d",
    "db:setup:prod": "docker-compose --env-file .env.production up -d",
    "db:setup:staging": "docker-compose --env-file .env.staging up -d",
    "db:migration:generate": "yarn build && ts-node node_modules/typeorm/cli.js migration:generate -f ormconfig -d src/databases/migrations -n",
    "db:migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert -f ormconfig",
    "db:migration:create": "ts-node node_modules/typeorm/cli.js migration:create -f ormconfig -d src/databases/migrations -n",
    "db:migration:run": "ts-node node_modules/typeorm/cli.js migration:run"
  },
  "dependencies": {
    "@nestjs-architects/typed-cqrs": "^0.1.1",
    "@nestjs/common": "^7.4.4",
    "@nestjs/config": "^1.0.0",
    "@nestjs/core": "^7.4.4",
    "@nestjs/cqrs": "^7.0.1",
    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/platform-fastify": "^7.4.4",
    "@nestjs/swagger": "^4.6.0",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/typeorm": "^7.1.4",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "fastify-cookie": "^5.3.1",
    "fastify-csrf": "^3.1.0",
    "fastify-helmet": "^5.0.3",
    "fastify-swagger": "^3.3.0",
    "mysql2": "^2.2.5",
    "nestjs-redis": "^1.3.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.2.34"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tranducminh/generate-nest-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/tranducminh/generate-nest-boilerplate/issues"
  },
  "homepage": "https://github.com/tranducminh/generate-nest-boilerplate#readme",
  "keys": [
    "generator",
    "boilerplate",
    "nestjs",
    "nest",
    "typescript",
    "node",
    "nodejs",
    "starter",
    "cli"
  ]
}